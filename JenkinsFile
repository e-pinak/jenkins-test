pipeline {
    agent any 
    stages {
        stage('Remote Server Deployment') {
            steps {
                    sh 'echo "cd /var/www/passfolio/pfa-laravel/ \n ./run.sh" > script.sh'
                    sh 'cat script.sh'
                    sh 'cat script.sh | ssh cears@192.168.0.137'
            }
        }
    }
}