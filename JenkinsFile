pipeline {
    agent any 
    stages {
        stage('Zipping') {
            steps {
                sh './zip.sh'
            }
        }
        stage('Deploy') {
            steps {
                sh './deploy.sh'
            }
        }
        stage('Build') {
            steps {
                sh './script/build.sh'
            }
        }
        stage('Clear') {
            steps {
                sh './clear.sh'
            }
        }
    }
} 